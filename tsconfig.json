{
	"compilerOptions": {
		"target": "es2020",
		"lib": [
			"es2020",
			"webworker"
		],
		"alwaysStrict": true,
		"strict": true,
		"preserveConstEnums": true,
		"moduleResolution": "node",
		"sourceMap": true,
		"esModuleInterop": true,
		"types": [
			"@cloudflare/workers-types"
		],
		"resolveJsonModule": true
	},
	"include": [
		"./src/*.ts",
		"./src/routes/*.ts",
		"./src/utils/*.ts",
		"./node_modules/@cloudflare/workers-types/index.d.ts"
],
	"exclude": [
		"node_modules/",
		"dist/"
	]
}